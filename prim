#include <iostream>
using namespace std;
int main()
{
int n,d,prim;
cin>>n;
prim=1;
if(n<2)
prim=0;
for(d=2;d*d<=n;d++)
if(n%d==0)
{
prim=0;
break;
}
if(prim==1)
cout<<n<<" este prim ";
else
cout<<n<<" nu este prim ";
return 0;
}

pt fct
int nr_prim(int x)
{

    if(x<2)
        return 0;
    for(int d=2; d*d<=x; d++)
        if(x%d==0)
    return 0;

    return 1;
}
